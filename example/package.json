{
  "name": "react-native-brotli-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "android:tgz": "BROTLI_SOURCE=tgz react-native run-android",
    "ios:tgz": "BROTLI_SOURCE=tgz react-native run-ios",
    "start:tgz": "BROTLI_SOURCE=tgz react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --mode Debug",
    "build:android:tgz": "BROTLI_SOURCE=tgz react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios:tgz": "BROTLI_SOURCE=tgz react-native build-ios --mode Debug"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-brotli": "file:../react-native-brotli-0.1.0.tgz",
    "react-native-fs": "^2.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/metro-config": "0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@types/react": "^19.2.0",
    "react-native-builder-bob": "^0.40.18",
    "react-native-monorepo-config": "^0.3.3"
  },
  "engines": {
    "node": ">=20"
  }
}
